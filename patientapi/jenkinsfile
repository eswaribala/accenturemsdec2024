#!groovy
pipeline{
 agent any
  tools {
    maven 'maven'
  }
 stages{
   stage("build"){
       steps{
          bat "mvn -f ./patientapi clean install -DskipTests"
          }
      }
   stage("dockerbuild"){
          steps{
             bat "cd patientapi & docker build -f dockerfile -t patientapiv15 ."
             }
         }
  }
 }